<input>
    <keyboard>
        <key n="127">
            <name>Delete</name>
            <desc>Toggle Reversers</desc>
            <binding>
                <condition>
                    <equals>
                        <property>controls/engines/engine[0]/throttle</property>
                        <value>0</value>
                    </equals>
                </condition>
                <command>property-toggle</command>
                <property>controls/engines/engine[0]/reverser</property>
            </binding>
            <binding>
                <condition>
                    <equals>
                        <property>controls/engines/engine[1]/throttle</property>
                        <value>0</value>
                    </equals>
                </condition>
                <command>property-toggle</command>
                <property>controls/engines/engine[1]/reverser</property>
            </binding>
        </key>
        <key n="79">
            <name>O</name>
            <desc>Open aerotow hook</desc>
            <binding>
                <command>property-assign</command>
                <property>sim/hitches/aerotow/open</property>
                <value type="bool">true</value>
            </binding>
        </key>
        <key n="111">
            <name>o</name>
            <desc>Lock aerotow hook</desc>
            <binding>
                <command>property-assign</command>
                <property>sim/hitches/aerotow/open</property>
                <value type="bool">false</value>
            </binding>
        </key>
	<key n="106">
            <name>j</name>
            <desc>Push the left condition</desc>
            <repeatable type="bool">true</repeatable>
            <binding>
              <command>property-adjust</command>
              <property>controls/engines/engine[0]/condition</property>            <step>0.05</step>
	      <min>0</min>
	      <max>1</max>
            </binding>
        </key>
	<key n="107">
            <name>k</name>
            <desc>Push the right condition</desc>
            <repeatable type="bool">true</repeatable>
            <binding>
              <command>property-adjust</command>
              <property>controls/engines/engine[1]/condition</property>            <step>0.05</step>
	      <min>0</min>
	      <max>1</max>
            </binding>
        </key>
	<key n="74">
            <name>J</name>
            <desc>Pull the left condition</desc>
            <repeatable type="bool">true</repeatable>
            <binding>
              <command>property-adjust</command>
              <property>controls/engines/engine[0]/condition</property>            <step>-0.05</step>
	      <min>0</min>
	      <max>1</max>
            </binding>
        </key>
	<key n="75">
            <name>K</name>
            <desc>Pull the right condition</desc>
            <repeatable type="bool">true</repeatable>
            <binding>
              <command>property-adjust</command>
              <property>controls/engines/engine[1]/condition</property>            <step>-0.05</step>
	      <min>0</min>
	      <max>1</max>
            </binding>
        </key>
    </keyboard>
