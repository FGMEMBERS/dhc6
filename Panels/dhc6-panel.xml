<!DOCTYPE PropertyList SYSTEM "panel.dtd">

<PropertyList>

	<name>de Havilland Canada DHC-6 Twin Otter IFR Panel</name>
	<background>Aircraft/dhc6/Panels/panel.png</background>
	<w>1280</w>
	<h>800</h>

	<instruments>

		<!-- PRIMARY FLIGHT INSTRUMENTS -->

		<!-- Airspeed indicator -->
		<instrument include="Aircraft/dhc6/Panels/Instruments/asi/asi.xml">
			<name>Airspeed Indicator</name>
			<x>110</x>
			<y>530</y>
			<w>210</w>
			<h>210</h>
		</instrument>

		<!-- Artifical Horizon -->
		<instrument include="Aircraft/Instruments/attitude-indicator-hi.xml">
			<name>Artifical Horizon</name>
			<x>325</x>
			<y>530</y>
			<w>210</w>
			<h>210</h>
		</instrument>

		<!-- Altimeter -->
		<instrument include="Aircraft/dhc6/Panels/Instruments/alt/alt.xml">
			<name>Altimeter</name>
			<x>540</x>
			<y>530</y>
			<w>210</w>
			<h>210</h>
		</instrument>

		<!-- Turn Coordinator -->
		<instrument include="Aircraft/dhc6/Panels/Instruments/tc/tc.xml">
			<name>Turn Coordinator</name>
			<x>110</x>
			<y>315</y>
			<w>210</w>
			<h>210</h>
		</instrument>

		<!-- Heading indicator -->
		<instrument include="Aircraft/dhc6/Panels/Instruments/hi/hi.xml">
			<name>Heading Indicator</name>
			<x>325</x>
			<y>315</y>
			<w>210</w>
			<h>210</h>
		</instrument>

		<!-- Variometer -->
		<instrument include="Aircraft/dhc6/Panels/Instruments/vsi/vsi.xml">
			<name>Variometer</name>
			<x>540</x>
			<y>315</y>
			<w>210</w>
			<h>210</h>
		</instrument>

		<!-- NAV INSTRUMENTS -->

		<!-- VOR Indicator -->

		<!-- RMI -->

		<!-- ENGINE INSTRUMENTS -->

	</instruments>

</PropertyList>