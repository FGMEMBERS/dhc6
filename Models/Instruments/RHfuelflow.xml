<?xml version="1.0"?>
<!-- RH fuel flow 
Syd Adams
-->
<PropertyList>

    <path>fuelflow.ac</path>
    
    <animation>
        <type>material</type>
        <object-name>FFface</object-name>
        <object-name>FFneedle</object-name>
        <emission>
            <red>0.028</red>
            <green>0.014</green>
            <blue>0.007</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FFneedle</object-name>
        <property>engines/engine[1]/fuel-flow_pph</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>100.0</ind><dep>30.0</dep></entry>
            <entry><ind>500</ind><dep>255.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>