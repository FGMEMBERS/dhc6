<?xml version="1.0"?>
<!-- prop percent rpm
Syd Adams
-->
<PropertyList>

    <path>prop-rpm.ac</path>
    
    <animation>
        <type>material</type>
        <object-name>PropRPM.face</object-name>
        <object-name>Prpm.1</object-name>
        <object-name>Prop.ind</object-name>
        <object-name>PropRPMneedle</object-name>
        <emission>
            <red>0.028</red>
            <green>0.014</green>
            <blue>0.007</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>PropRPMneedle</object-name>
        <property>engines/engine[1]/rpm</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>2200</ind><dep>270</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>